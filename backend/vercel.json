{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/api/forgotPassword",
      "methods": ["POST"],
      "dest": "./routes/route.js"
    },
    {
      "src": "/api/resetPassword",
      "methods": ["POST"],
      "dest": "./routes/route.js"
    },
    {
      "src": "/.*",
      "dest": "server.js"
    }
  ]
}
